{
    "projection-method": "perspective",
    "camera-focus": [0.0, 0.0, 0.0],
    "camera-distance": 20.0,
    "camera-theta": 0.0,
    "camera-phi": 90.0,
    "camera-rotation": 0.0,
    "aspect-ratio": 1.33333333,

    "light-location": [0.0, 0.0, 1.0],
    "light-focus": [0.0, 0.0, 0.0],
    "light-size": 2.0,
    "light-number": 5,
    "light-rotation": 0.0,
    "light-colour": "White",
    "light-adaptive": -1,
    "light-jitter": true,

    "background-colour": "",
    "draw-axes": false,
    "axes-length": 2.0,
    "axes-radius": 0.05,

    "element-textures":{
        "default": {
            "texture": [],
            "texture...prototype": "wood",
            "pigment": [],
            "pigment...prototype": "blue",
            "use-colour": true,
            "normal": [],
            "normal...prototype": "bumps",
            "finish": ["specular 0.5", "metallic"],
            "use-colour": true
            }
        },
    "element-textures...update": "extend",
    "element-textures...prototype-key": "default",

    "element-colour-scheme": "Jmol",
    "element-colour-change": {},
    "element-colour-change...update": "extend",
    "element-colour-change...prototype": "red",

    "element-radii": {
        "default": 0.4
    },
    "element-radii...update": "extend",
    "element-radii...prototype-key": "default",

    "compute-bonds": false,

    "covalent-radii": {},
    "covalent-radii...update": "extend",
    "covalent-radii...prototype": 1.0,

    "multiple-bond-separation": 0.12,
    "partial-bond-dash-size": 0.1,
    "bond-cylinder-radius": 0.03,
    "bond-texture": [],
    "bond-texture...prototype": "wood",
    "bond-pigment": ["Dark_Wood", "scale 0.1"],
    "bond-pigment...prototype": "wood",
    "bond-normal": [],
    "bond-normal...prototype": "bumps",
    "bond-finish": [],
    "bond-finish...prototype": "metallic",

    "pov-ray-program": "povray",
    "graph-width": 1024,
    "quality": 5,
    "suppress-povray-out": true,
    "additional-printing": false
}
